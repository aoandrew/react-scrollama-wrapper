  },
  "peerDependencies": {
    "react": "17.x || 18.x",
    "react-dom": "17.x || 18.x",
    "scrollama": "3.x"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "gh-pages": "^3.2.3",
    "rollup": "^2.59.0",
    "rollup-plugin-peer-deps-external": "^2.2.4"
