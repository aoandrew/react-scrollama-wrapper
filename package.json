{
  "name": "@ap.cx/react-scrollama-wrapper",
  "version": "3.0.3",
  "private": false,
  "description": "react scrollama wrapper",
  "main": "lib/index.js",
  "module": "lib/index.module.js",
  "types": "index.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
